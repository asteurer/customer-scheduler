<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="steurer.software_two.controller.LogInController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="325.0" layoutY="174.0" prefHeight="250.0" prefWidth="250" spacing="15.0">
         <children>
            <Label fx:id="logInTitleLbl" text="Log In">
               <font>
                  <Font name="System Bold" size="27.0" />
               </font></Label>
            <GridPane maxWidth="150.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="130.0" minWidth="130.0" prefWidth="130.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label fx:id="usernameFieldLbl" text="Username:" />
                  <Label fx:id="passwordFieldLbl" text="Password:" GridPane.rowIndex="1" />
                  <TextField fx:id="usernameField" onAction="#onActionUsernameField" GridPane.columnIndex="1" />
                  <PasswordField fx:id="passwordField" onAction="#onActionPasswordField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <HBox alignment="CENTER_RIGHT" prefHeight="30.0" prefWidth="200.0">
               <children>
                  <Button fx:id="goBtn" mnemonicParsing="false" onAction="#onActionGoBtn" text="Go" />
               </children>
               <padding>
                  <Insets right="25.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER" prefHeight="30.0" prefWidth="200.0">
               <children>
                  <Label fx:id="locationLbl" text="Location:" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
